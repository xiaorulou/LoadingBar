<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dyhdyh.loadingbar.example"
    android:versionCode="8"
    android:versionName="1.4.6" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.dyhdyh.loadingbar.example.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dyhdyh.loadingbar.example.ListLoadingActivity" />
        <activity
            android:name="com.dyhdyh.loadingbar.example.LoadingBarActivity"
            android:label="LoadingBar" />
        <activity
            android:name="com.dyhdyh.loadingbar.example.LoadingDialogActivity"
            android:label="LoadingDialog" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.dyhdyh.loadingbar.example.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>